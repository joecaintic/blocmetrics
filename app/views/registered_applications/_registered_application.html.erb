<%= content_tag :div, class: 'media', id: "registered_application-#{registered_application.id}" do %>
  <div class = "row">
    <div class="col-md-8">
      <p><%= registered_application.name %>
      <% if policy(registered_application).destroy? %>
        <small><%= link_to "Delete", [registered_application.user, registered_application], method: :delete, remote: true %></small> </p>
      <% end %>
    </div>
  </div>
<% end %>
